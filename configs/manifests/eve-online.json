{
  "schema": "elm.manifest.v1",
  "id": "eve-online",
  "display_name": "EVE Online",
  "installer": {
    "type": "launcher",
    "source": {
      "url": "https://binaries.eveonline.com/EveLauncher-2180591.exe"
    },
    "install_dir": "CCP/EVE"
  },
  "engine": {
    "ref": "ge-proton10-27"
  },
  "runtime": {
    "wineprefix_layout": "per-profile",
    "dx": {
      "preferred": "dx11",
      "allow_dx12": true
    },
    "components": {
      "dxvk": {
        "enabled": true
      },
      "vkd3d": {
        "enabled": true
      }
    }
  },
  "env": {
    "base": {
      "DXVK_ASYNC": "1",
      "PROTON_NO_ESYNC": "1",
      "PROTON_NO_FSYNC": "1",
      "DXVK_HUD": "fps,frametimes",
      "WINE_FULLSCREEN_FSR": "1"
    }
  },
  "launch": {
    "entrypoints": [
      {
        "name": "EVE Launcher",
        "type": "exe",
        "path": "drive_c/users/steamuser/AppData/Local/eve-online/eve-online.exe",
        "args": []
      }
    ]
  }
}
